{
  "buildCommand": "cd home && npm run build",
  "outputDirectory": "home/dist",
  "installCommand": "cd home && npm install",
  "framework": null,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "jerret.me"
        }
      ],
      "destination": "https://blog.jerret.me/:path*",
      "permanent": false
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.jerret.me"
        }
      ],
      "destination": "https://blog.jerret.me/:path*",
      "permanent": false
    }
  ]
}
