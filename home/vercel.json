{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "jerret.me"
        }
      ],
      "destination": "https://blog.jerret.me/:path*",
      "permanent": false
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.jerret.me"
        }
      ],
      "destination": "https://blog.jerret.me/:path*",
      "permanent": false
    }
  ]
}
